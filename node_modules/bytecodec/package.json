{
  "name": "bytecodec",
  "version": "2.0.0",
  "description": "Zero-dependency byte toolkit for base64url, UTF-8 strings, JSON, BufferSource helpers, CSP-ready nonces, gzip, and timing-safe equality in browser and Node runtimes.",
  "keywords": [
    "base64url",
    "base64",
    "Uint8Array",
    "buffersource",
    "arraybuffer",
    "bytes",
    "encoding",
    "decoding",
    "browser",
    "node",
    "utf8",
    "json",
    "nonce",
    "csp",
    "equals",
    "timing-safe",
    "compression",
    "gzip",
    "secure-compare",
    "binary"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jortsupetterson/bytecodec.git"
  },
  "bugs": {
    "url": "https://github.com/jortsupetterson/bytecodec/issues"
  },
  "homepage": "https://github.com/jortsupetterson/bytecodec#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "npm run build && node test.js && node benchmark/bench.js",
    "bench": "npm run build && node benchmark/bench.js",
    "build": "tsc -p tsconfig.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.6.3"
  }
}
