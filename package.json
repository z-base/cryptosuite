{
  "name": "zeyra",
  "version": "3.0.0",
  "description": "JS/TS WebCrypto toolkit for encrypt/decrypt, sign/verify, and wrap/unwrap",
  "keywords": [
    "webcrypto",
    "aes-gcm",
    "ecdsa",
    "hmac",
    "rsa-oaep",
    "jwk",
    "encryption",
    "signature",
    "key-wrapping",
    "wrapping",
    "unwrapping",
    "cluster",
    "keyset",
    "compression",
    "bytecodec",
    "typescript",
    "z-base"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node --test test.js && node test.js --bench",
    "bench": "npm run build && node test.js --bench",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-base/zeyra.git"
  },
  "bugs": {
    "url": "https://github.com/z-base/zeyra/issues"
  },
  "homepage": "https://github.com/z-base/zeyra#readme",
  "dependencies": {
    "bytecodec": "^3.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.0.10",
    "c8": "^10.1.3",
    "playwright": "^1.58.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false
}
