{
  "name": "@z-base/cryptosuite",
  "version": "1.0.1",
  "description": "Developer-experience-first WebCrypto toolkit with declarative APIs for encryption, signing, key wrapping, and identifiers.",
  "keywords": [
    "cryptography",
    "webcrypto",
    "crypto",
    "encryption",
    "decryption",
    "signing",
    "verification",
    "key-wrap",
    "key-unwrap",
    "jwk",
    "aes-gcm",
    "hmac",
    "ed25519",
    "rsa-oaep",
    "opaque-identifier",
    "typescript",
    "bytecodec",
    "z-base"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier . --write",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node test/run-coverage.mjs && node test/e2e/run.mjs",
    "bench": "npm run build && node benchmark/bench.js",
    "prepare": "husky",
    "prepublishOnly": "npm run test && npm run bench"
  },
  "c8": {
    "all": true,
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.d.ts",
      "test/**",
      "benchmark/**",
      "playwright.config.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-base/cryptosuite.git"
  },
  "bugs": {
    "url": "https://github.com/z-base/cryptosuite/issues"
  },
  "homepage": "https://github.com/z-base/cryptosuite#readme",
  "dependencies": {
    "@z-base/bytecodec": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.0.10",
    "c8": "^10.1.3",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "playwright": "^1.58.0",
    "prettier": "^3.8.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false
}
